# Firebase Configuration - Client-side
# These need to be prefixed with NEXT_PUBLIC_ to be exposed to the browser.
# Find these values in your Firebase project settings.
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=

# Genkit/Google AI Configuration - Server-side
# This is used by Genkit and should NOT be exposed to the browser.
# Get this key from Google AI Studio.
GOOGLE_API_KEY=
